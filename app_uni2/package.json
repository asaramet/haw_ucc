{
  "name": "frontend",
  "version": "1.0.0",
  "description": "bwUniCluster stats",
  "main": "main.js",
  "bin": {
    "frontend": "app.js"
  },
  "scripts": {
    "clean:app": "rm -rf src/app/users/*",
    "clean:all": "find * -maxdepth 0 ! -name package.json -not -name app_scripts -not -name saves -not -name node_modules -not -name package-lock.json -print0 | xargs -0 rm -rf --",
    "clean:js": "find src/ -name '*.js' -type f -exec rm {} -f \\;",
    "clean:js.map": "find src/ -name '*.js.map' -type f -exec rm {} -f \\;",
    "clean:json": "find src/app/ -name '*.json' -type f -exec rm {} -f \\;",
    "clean:src": "npm run clean:js && npm run clean:js.map && npm run clean:json && rm -f src/main.aot.ts",
    "copy:new": "rsync -uavh --exclude package.json --exclude node_modules --exclude aot_files/htppwd ../bwUniCluster2/* .",
    "copy:app": "rsync -uavh --delete-excluded ../bwUniCluster1/src/app/* src/app/",
    "cp:aot": "cp aot_files/index.html aot_files/favicon.ico public/ && cp aot_files/main.aot.ts src/",
    "make:new": "npm run clean:all && npm run copy:new",
    "make": "npm run clean:all && npm run copy:new && npm run clean:app",
    "cp:css": "cp public/css/styles.css src/",
    "build": "npm run cp:aot && ngc -p tsconfig-prod-aot.json",
    "build:css": "webpack --config webpack/css.config.js && npm run cp:css",
    "create:new": "./app_scripts/create.sh -b",
    "watch": "ng serve --aot --watch",
    "update": "./app_scripts/update.sh"
  },
  "author": "Alexandru Saramet",
  "license": "ISC",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.801.1",
    "@angular/animations": "^8.1.1",
    "@angular/cdk": "^8.0.2",
    "@angular/cli": "^8.1.1",
    "@angular/common": "^8.1.1",
    "@angular/compiler": "^8.1.1",
    "@angular/compiler-cli": "^8.1.1",
    "@angular/core": "^8.1.1",
    "@angular/forms": "^8.1.1",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "^8.1.1",
    "@angular/platform-browser-dynamic": "^8.1.1",
    "@angular/router": "^8.1.1",
    "@ngtools/webpack": "^8.1.1",
    "@types/core-js": "^2.5.2",
    "angular-google-charts": "^0.1.6",
    "compression-webpack-plugin": "^3.0.0",
    "core-js": "^3.1.4",
    "css-loader": "^3.0.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "node-sass": "^4.12.0",
    "rxjs": "^6.5.2",
    "sass-loader": "^7.1.0",
    "typescript": ">=3.4 <3.5",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "zone.js": "^0.9.1"
  },
  "dependencies": {}
}
