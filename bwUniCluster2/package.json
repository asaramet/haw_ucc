{
  "name": "bwUniCluster2-stats",
  "version": "1.0.0",
  "description": "bwUniCluster stats",
  "main": "main.js",
  "bin": {
    "frontend": "app.js"
  },
  "repository": {},
  "scripts": {
    "clean:js": "find src/ -name '*.js' -type f -exec rm {} -f \\;",
    "clean:js.map": "find src/ -name '*.js.map' -type f -exec rm {} -f \\;",
    "clean:json": "find src/app/ -name '*.json' -type f -exec rm {} -f \\;",
    "clean:src": "npm run clean:js && npm run clean:js.map && npm run clean:json",
    "clean:public": "rm -rf public/*",
    "clean": "npm run clean:src && npm run clean:public",
    "clean:app": "rm -rf src/app",
    "cp:css": "cp public/css/styles.css src/",
    "build": "npm run clean && ng build --ts-config tsconfig-prod-aot.json",
    "build:css": "webpack --config webpack/css.config.js && npm run cp:css",
    "prod": "npm run build && npm run build:css && webpack --config webpack/aot.config.js",
    "compile": "test -d public || mkdir public; npm run prod && npm run clean:src",
    "sync": "chmod g=u public -R && rsync -uavrh public/ /www/faculty/it/bwHPC/uni2/all/ --delete-excluded",
    "save": "test -d public || test -d ./prod_work || rm ./prod_work -rf;  mv public prod_work || echo 'compile first!!!'",
    "watch": "ng serve --watch",
    "update": "scripts/update.sh && npm run compile",
    "rsync": "npm run sync && npm run save"
  },
  "author": "Alexandru Saramet",
  "license": "ISC",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular/animations": "^11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/cli": "^11.0.5",
    "@angular/common": "^11.0.5",
    "@angular/compiler": "^11.0.5",
    "@angular/compiler-cli": "^11.0.5",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^11.0.5",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "^11.0.5",
    "@angular/platform-browser-dynamic": "^11.0.5",
    "@angular/router": "^11.0.5",
    "@ngtools/webpack": "^11.0.5",
    "@types/core-js": "^2.5.4",
    "angular-google-charts": "^1.1.6",
    "compression-webpack-plugin": "^7.1.1",
    "core-js": "^3.8.2",
    "css-loader": "^5.0.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "postcss": "^8.2.2",
    "rxjs": "^6.6.3",
    "sass-loader": "^10.1.0",
    "typescript": "^4.0.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "zone.js": "^0.11.3"
  },
  "dependencies": {}
}
