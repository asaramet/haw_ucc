{
  "name": "frontend",
  "version": "1.0.0",
  "description": "bwUniCluster stats",
  "main": "main.js",
  "bin": {
    "frontend": "app.js"
  },
  "scripts": {
    "clean:js": "find src/ -name '*.js' -type f -exec rm {} -f \\;",
    "clean:js.map": "find src/ -name '*.js.map' -type f -exec rm {} -f \\;",
    "clean:json": "find src/app/ -name '*.json' -type f -exec rm {} -f \\;",
    "clean:src": "npm run clean:js && npm run clean:js.map && npm run clean:json && rm -f src/main.aot.ts",
    "clean:public": "rm -rf public/*",
    "clean": "npm run clean:src && npm run clean:public",
    "clean:app": "rm -rf src/app",
    "cp:aot": "cp aot_files/index.html aot_files/favicon.ico public/ && cp aot_files/main.aot.ts src/",
    "cp:css": "cp public/css/styles.css src/",
    "build": "npm run clean && npm run cp:aot && ngc -p tsconfig-prod-aot.json",
    "build:css": "webpack --config webpack/css.config.js && npm run cp:css",
    "prod": "npm run build && npm run build:css && webpack --config webpack/aot.config.js",
    "compile": "test -d public || mkdir public; npm run prod && npm run clean:src",
    "sync": "chmod g=u public -R && rsync -uavrh public/ /www/faculty/it/bwHPC/_ssl/all/ --delete-excluded",
    "save": "test -d public || test -d ./prod_work || rm ./prod_work -rf;  mv public prod_work || echo 'compile first!!!'",
    "watch": "ng serve --aot --watch",
    "update": "main_sh/update.sh && npm run compile",
    "rsync": "npm run sync && npm run save"
  },
  "author": "Alexandru Saramet",
  "license": "ISC",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.26",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "^8.3.26",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ngtools/webpack": "^8.3.26",
    "@types/core-js": "^2.5.3",
    "angular-google-charts": "^1.1.4",
    "compression-webpack-plugin": "^3.1.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "extract-loader": "^5.0.1",
    "file-loader": "^5.1.0",
    "html-loader": "^1.1.0",
    "node-sass": "^4.14.0",
    "rxjs": "^6.5.5",
    "sass-loader": "^8.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "typescript": "^3.5.3",
    "zone.js": "^0.9.1"
  }
}
