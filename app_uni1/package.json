{
  "name": "frontend",
  "version": "1.0.0",
  "description": "bwUniCluster stats",
  "main": "main.js",
  "bin": {
    "frontend": "app.js"
  },
  "scripts": {
    "clean:app": "rm -rf src/app/users/*",
    "clean:all": "find * -maxdepth 0 ! -name package.json -not -name node_modules -not -name app_scripts -not -name saves -not -name package-lock.json -print0 | xargs -0 rm -rf --",
    "clean:js": "find src/ -name '*.js' -type f -exec rm {} -f \\;",
    "clean:js.map": "find src/ -name '*.js.map' -type f -exec rm {} -f \\;",
    "clean:json": "find src/app/ -name '*.json' -type f -exec rm {} -f \\;",
    "clean:src": "npm run clean:js && npm run clean:js.map && npm run clean:json && rm -f src/main.aot.ts",
    "copy:new": "rsync -uavh --exclude package.json --exclude package-* --exclude aot_files/htppwd --exclude node_modules ../bwUniCluster1/* .",
    "copy:app": "rsync -uavh --delete-excluded ../bwUniCluster1/src/app/* src/app/",
    "cp:aot": "cp aot_files/index.html aot_files/favicon.ico public/ && cp aot_files/main.aot.ts src/",
    "make:new": "npm run clean:all && npm run copy:new",
    "make": "npm run clean:all && npm run copy:new && npm run clean:app",
    "cp:css": "cp public/css/styles.css src/",
    "build": "npm run cp:aot && ngc -p tsconfig-prod-aot.json",
    "build:css": "webpack --config webpack/css.config.js && npm run cp:css",
    "prod": "npm run build && npm run build:css && webpack --config webpack/aot.config.js",
    "compile": "test -d public || mkdir public; npm run prod && npm run clean:src",
    "create:new": "./app_scripts/create.sh -b",
    "watch": "ng serve --aot --watch",
    "update": "./app_scripts/update.sh"
  },
  "author": "Alexandru Saramet",
  "license": "ISC",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.26",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "^8.3.26",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ngtools/webpack": "^8.3.26",
    "@types/core-js": "^2.5.3",
    "angular-google-charts": "*",
    "compression-webpack-plugin": "^3.1.0",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "extract-loader": "^5.0.1",
    "file-loader": "^5.1.0",
    "html-loader": "^1.1.0",
    "node-sass": "^4.13.1",
    "rxjs": "^6.5.5",
    "sass-loader": "^8.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "typescript": "^3.5.3",
    "zone.js": "^0.9.1"
  }
}
